{
  "name": "apogey",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:server": "tsx watch server/api.ts",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "dev:tunnel": "concurrently \"npm run dev:server\" \"npm run dev\" \"npm run tunnel\"",
    "tunnel": "node scripts/tunnel.js",
    "tunnel:ngrok": "concurrently \"npm run dev:server\" \"npm run dev\" \"ngrok http 5173\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "server": "tsx server/api.ts"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "localtunnel": "^2.0.2",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.15",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vite": "^6.0.4"
  },
  "overrides": {
    "axios": "^1.7.9"
  }
}



